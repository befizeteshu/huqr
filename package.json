{
  "name": "huqr",
  "version": "1.1.7",
  "description": "Implementation of the hungarian qr code standard",
  "keywords": [],
  "main": "dist/huqr.umd.js",
  "browser": "dist/huqr.umd.js",
  "module": "dist/huqr.es5.js",
  "typings": "dist/types/huqr.d.ts",
  "files": [
    "dist"
  ],
  "author": "Gabor Dolla <gabor.dolla@befizetes.hu>",
  "repository": {
    "type": "git",
    "url": "https://github.com/befizeteshu/huqr"
  },
  "license": "MIT",
  "engines": {
    "node": ">=13.0.0"
  },
  "scripts": {
    "lint": "eslint src/* test/*",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/iban": "0.0.32",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-config": "^29.3.1",
    "lint-staged": "^13.1.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "date-fns": "^2.19.3",
    "date-fns-tz": "^1.3.7",
    "iban": "0.0.14",
    "joi": "^17.7.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "verbose": true
  }
}
